 content = file.read() 
 print(content)

f = 'aq.py'
with open('aq.py', 'r') as file: 
 count = 0 
 for line in file:
     count += 1 
print("Number of lines in the file:", count)

f = 'aq.py'
with open('aq.py', 'r') as file: 
 word_count = 0 
 for line in file:
     words = line.split()
     word_count += len(words) 
print("Number of words in the file:", word_count)

file_path = 'aq.py'
with open('aq.py', 'r') as file: 
 char_count = 0 
 for line in file:
     char_count += len(line)   
 print("Number of characters ", char_count)
 
source_file_path = 'aq.py' 
destination_file_path = 'a.py' 
with open(source_file_path, 'r') as source_file: 
 with open(destination_file_path, 'w') as destination_file:
     for line in source_file:
         destination_file.write(line) 
print("File copied successfully!")

file_path = 'aq.py'
strings_list = ["Hello", "World", "Python", "Programming"] 
with open(file_path, 'w') as file: 
 for string in strings_list:
     file.write(string + "\n")
print("List of strings has been written to the fileÂ successfully!")
